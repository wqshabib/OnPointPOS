@model ProfileViewModel
@{
    ViewBag.Title = "Detail";
    Layout = null;
}

<div class="panel">
    <div class="panel-heading"> <span class="panel-title"> <span class="glyphicon glyphicon-info-sign"></span>User Detail </span> </div>
    <div class="panel-body">



        <form class="form-horizontal" role="form">

            <div class="form-group">
                <label class="col-lg-2 control-label">First Name</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.FirstName</p>
                </div>
            
                <label class="col-lg-2 control-label">Last Name</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.LastName</p>
                </div>
            </div>

            <div class="form-group">

                <label class="col-lg-2 control-label">Nick Name</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.NickName</p>
                </div>

                <label class="col-lg-2 control-label">Email</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.Email</p>
                </div>


              
           
               
            </div>

            <div class="form-group">
                <label class="col-lg-2 control-label">Mobile No</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.MobileNo</p>
                </div>
         
                <label class="col-lg-2 control-label">City</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.City</p>
                </div>
            </div>

            <div class="form-group">
                <label class="col-lg-2 control-label">Address</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.Address</p>
                </div>
           
                <label class="col-lg-2 control-label">Zip Code</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.ZipCode</p>
                </div>
            </div>

            <div class="form-group">
                <label class="col-lg-2 control-label">Country</label>
                <div class="col-lg-4">
                    <p class="form-control-static text-muted">@Model.Country</p>
                </div>
            </div>


            </form>

    </div>

</div>